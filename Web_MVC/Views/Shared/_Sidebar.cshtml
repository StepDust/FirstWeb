@model string[,]


<div id="Sidebar" style="display:none;">
    <ul>
        @{
            int temp = 0;
            for (int i = 0; i < Model.Length / 2; i++) {
                <li>
                    <a href="#@Model[i, 1]" class="com-a-block com-a-block-anim">
                        @{temp = i + 1; }
                        @(temp >= 10 ? temp + "" : "0" + temp)、@Model[i, 0]
                    </a>
                </li>
            }
        }
    </ul>
</div>